/***** End of imports. If edited, may not auto-convert in the playground. *****/
Map.addLayer(geometry,{},'Kajiado North');
var band=['B4','B3','B2','B8'];
var bandL7=['B1','B2','B3'];
var filtered=ee.Image(S2.filterDate('2020-01-01','2021-06-01').filterBounds(geometry).select(band).sort("CLOUDY_PIXEL_PERCENTAGE").first());
print(filtered);
Map.addLayer(filtered,{min:0,max:3000},'sentinel');
// Export.image.toDrive({image:filtered,region:geometry, scale:10});
var clip=filtered.clip(geometry);
Map.addLayer(clip,{min:0,max:3000},'sentinel Kajiado');
// Export.image.toDrive({image:clip,scale:10,region:geometry});
var training_data=urban_areas.merge(grasslands).merge(forest).merge(roads);
var training=filtered.select(band)
      .sampleRegions({collection:training_data, properties:['LC'], scale:10});
var classifier=ee.Classifier.smileCart()
      .train({features:training, classProperty:'LC', inputProperties:band});
var classified2020=filtered.select(band).classify(classifier);
var clip=classified2020.clip(geometry);
Map.addLayer(clip,{palette:'red,yellow,green,grey',min:1,max:4},'landcover kajiado 2020 Oct');
var filtered1999=ee.Image(L7.filterDate('2000-01-01','2000-12-31').filterBounds(geometry).sort('CLOUD_COVER').first());
print(filtered1999);
var clipL7=filtered1999.clip(geometry);
Map.addLayer(clipL7,{bands:['B3','B2','B1'],min:0,max:0.7},'Nairobi 1999 nov 1');
Map.addLayer(filtered1999,{bands:['B3','B2','B1'],min:0,max:0.7},'Kajiado 1999 nov 1');
var training_data00=urban_areas00.merge(unDeveloped_Areas00).merge(water00).merge(forest00);
print(training_data00);
var training00=filtered1999.select(bandL7)
      .sampleRegions({collection:training_data00, properties:['LC'], scale:30});
print(training00);
var classifier00=ee.Classifier.smileCart()
      .train({features:training00, classProperty:'LC', inputProperties:bandL7});
var classified00=filtered1999.select(bandL7).classify(classifier00);
print(classified00);
var clip00=classified00.clip(geometry);
Map.addLayer(clip00,{palette:'red,yellow,green,grey',min:1,max:4},'landcover kajiado 2000');