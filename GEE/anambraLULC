    LC = ee.ImageCollection("ESA/WorldCover/v100");
/***** End of imports. If edited, may not auto-convert in the playground. *****/
var filtered2002=L7.filterDate('2002-1-1','2002-12-31').filterBounds(Onitsha).sort('CLOUD_COVER').first();
print(filtered2002);
var bands=['B1','B2','B3','B4','B5'];
var bands2=['B2','B3','B4','B8'];
var vizparamsTrue={bands:['B3','B2','B1'],max:0.8,min:0,gamma:[0.95,1.1,1]};
var vizparamsFalse={bands:['B4','B3','B2'],max:0.8,min:0,gamma:[0.95,1.1,1]};

var vizparamsTrue8={bands:['B4','B3','B2'],max:0.8,min:0,gamma:[0.95,1.1,1]};
var vizparamsFalse8={bands:['B5','B4','B3'],max:0.8,min:0,gamma:[0.95,1.1,1]};

Map.setCenter(6.79, 6.17,12);
Map.addLayer(filtered2002,vizparamsTrue,'2002 rgb');
Map.addLayer(filtered2002,vizparamsFalse,'2002 false');
var ONClip=filtered2002.clip(Onitsha);
Map.addLayer(ONClip,vizparamsTrue,'ONCLip 2002 rgb');
Map.addLayer(ONClip,vizparamsFalse,'ONCLip 2002 false');

//2002 classification
//merge training data
var merged_fc02=BuiltUp02.merge(Vegetation02).merge(BareLand02).merge(Water02);
//define training feature classes, property and the resolution of image
var training02=ONClip.sampleRegions({
  collection:merged_fc02,
  properties:['LC02'],
  scale:30
});
//define classifier
var classifier02=ee.Classifier.smileCart();
//define training parameters
var classifier02=classifier02.train({
  features:training02,
  classProperty:'LC02',
  inputProperties:bands
});
//classify image
var classified2002=ONClip.classify(classifier02);
print(classified2002);
//display image
Map.addLayer(classified2002,{min:0,max:3,palette:'red,green,#feffa1,blue'},'classified2002');
//2012 LULC
//obtain image with the lowest cloud cover
var filtered2012=L7.filterDate('2012-1-1','2012-12-31').filterBounds(Onitsha).sort('CLOUD_COVER').first();
print(filtered2012);
//rectify scan line error
var fill2012=filtered2012.focalMean(1,'square','pixels',20);
var final2012=fill2012.blend(filtered2012);
//display rectified image
Map.addLayer(final2012,vizparamsTrue,'2012 rgb');
Map.addLayer(final2012,vizparamsFalse,'2012 false');
//clip image to area of study
var ONClip12=final2012.clip(Onitsha);
Map.addLayer(ONClip12,vizparamsTrue,'ONCLip 2012 rgb');
Map.addLayer(ONClip12,vizparamsFalse,'ONCLip 2012 false');

//2012 classification
//merge training data
var merged_fc12=BuiltUp12.merge(Vegetation12).merge(BareLand12).merge(Water12);
//define training feature classes, property and the resolution of image
var training12=ONClip12.sampleRegions({
  collection:merged_fc12,
  properties:['LC12'],
  scale:30
});
//define classifier
var classifier12=ee.Classifier.smileCart();
//define training parameters
var classifier12=classifier12.train({
  features:training12,
  classProperty:'LC12',
  inputProperties:bands
});
//classify image
var classified2012=ONClip12.classify(classifier12);
print(classified2012);
//display image
Map.addLayer(classified2012,{min:0,max:3,palette:'red,green,#feffa1,blue'},'classified2012');

print(landcover);
var filtered2020=LC.first().clip(Onitsha);
print(filtered2020);
Map.addLayer(filtered2020,{},'landcover 2020');
var filtered20=L8.filterDate('2020-1-1','2020-12-31').filterBounds(Onitsha).sort('CLOUD_COVER').first().clip(Onitsha);
print(filtered20);

Map.addLayer(filtered20,vizparamsTrue8,'ONCLip 2020 rgb');
Map.addLayer(filtered20,vizparamsFalse8,'ONCLip 2020 false');

