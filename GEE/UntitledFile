var filtered2020=L8.filterBounds(geometry).filterDate('2020-01-01','2020-12-31').sort('CLOUD_COVER').first();
print(filtered2020);
var bands=['B5','B4','B3','B2'];
var vizparamTrue={
  bands:['B4','B3','B2'],
  min:0,max:0.5,
  gamma:[0.95,1.1,1]
};
var vizparamFalse={
  bands:['B5','B4','B3'],
  min:0,max:0.5,
  gamma:[0.95,1.1,1]
};
var KNClip2020=filtered2020.clip(KajiadoNorth);
Map.addLayer(filtered2020,vizparamTrue,'true colour 2000');
Map.addLayer(filtered2020,vizparamFalse,'false colour 2000');
Map.addLayer(KNClip2020,vizparamTrue,'KN true colour 2000');
Map.addLayer(KNClip2020,vizparamFalse,'KN false colour 2000');
//Classification 2020

var merged_fc20=BuiltUp20.merge(ForestedAreas20).merge(Vegetation20).merge(NonBuilt_Up20).merge(Water20);
//define training feature classes, property and the resolution of image
var training20=filtered2020.sampleRegions({
  collection:merged_fc20,
  properties:['LC20'],
  scale:30
});
//define classifier
var classifier=ee.Classifier.smileCart();
//define training parameters
var classifier20=classifier.train({
  features:training20,
  classProperty:'LC20',
  inputProperties:bands
});
//classify image
var classified2020=KNClip2020.classify(classifier20);
print(classified2020);
//display image
Map.addLayer(classified2020,{min:0,max:4,palette:'red,green,#87ff8a,#feffa1,blue'},'classified2020');
var filtered2018=L8.filterBounds(geometry).filterDate('2018-01-01','2018-12-31').sort('CLOUD_COVER').first();
var KNClip2018=filtered2018.clip(KajiadoNorth);
Map.addLayer(filtered2018,vizparamTrue,'true colour 2018');
Map.addLayer(filtered2018,vizparamFalse,'false colour 2018');
Map.addLayer(KNClip2018,vizparamTrue,'KN true colour 2018');
Map.addLayer(KNClip2018,vizparamFalse,'KN false colour 2018');

